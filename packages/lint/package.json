    "@antfu/eslint-config": "^2.6.2",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@commitlint/types": "^18.4.4",
    "@jannajs/tsconfig": "workspace:*",
    "@types/eslint": "^8.56.1",
    "@types/node": "^18.0.0",
    "eslint": "npm:eslint-ts-patch@^8.56.0-0",
    "eslint-ts-patch": "8.56.0-0",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
