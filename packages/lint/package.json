{
  "name": "@jannajs/lint",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsup src/eslint.ts src/config.ts src/configBin.ts src/index.ts --format cjs,esm --dts",
    "clean": "rimraf .turbo && rimraf node_modules && rimraf dist"
  },
  "files": [
    "dist",
    "bin"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "lint-config": "bin/config.js"
  },
  "dependencies": {
    "@umijs/fabric": "^2.10.2",
    "eslint": "^7",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0"
  },
  "devDependencies": {
    "@jannajs/tsconfig": "workspace:*",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.24",
    "@types/semver": "^7.3.9",
    "chalk": "^5.0.1",
    "typescript": "^4.6.3"
  }
}
